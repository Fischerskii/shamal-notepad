<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                            http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="init-person-schema" author="fischerski">
        <sql>
            create schema person;
        </sql>
    </changeSet>

    <changeSet id="create-person-person-id-seq" author="fischerski">
        <createSequence sequenceName="person_id_seq" schemaName="person"/>
    </changeSet>

    <changeSet id="create-person-address-id-seq" author="fischerski">
        <createTable tableName="person" schemaName="">
            <column name=""></column>
        </createTable>
    </changeSet>
